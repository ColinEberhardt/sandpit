{
  "name": "sandpit",
  "version": "0.0.17",
  "description": "A playground for creative coding using JavaScript and the canvas element",
  "main": "lib/Sandpit.js",
  "license": "MIT",
  "dependencies": {
    "@hughsk/fulltilt": "^0.7.1",
    "color": "^1.0.3",
    "dat.gui": "^0.6.1",
    "debounce": "^1.0.0",
    "gyronorm": "^2.0.4",
    "queryfetch": "^1.2.1",
    "seedrandom": "^2.4.2",
    "stats.js": "^0.17.0",
    "victor": "^1.1.0",
    "whatwg-fetch": "^2.0.1"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.18.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-latest": "^6.16.0",
    "canvas": "^1.6.2",
    "chokidar": "^1.6.1",
    "jsdoc": "^3.4.3",
    "minami": "git://github.com/superhighfives/minami.git",
    "pushstate-server": "^2.1.0",
    "react-scripts": "0.8.4",
    "surge": "^0.18.0",
    "three": "^0.83.0",
    "three-trackballcontrols": "^0.0.5",
    "webpack": "^1.14.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "test": "react-scripts test --env=jsdom",
    "serve": "npm run build && pushstate-server build",
    "build:lib": "rm -rf lib/* && babel --out-dir lib src",
    "build:watch": "npm run build:lib -- --watch",
    "build-docs": "rm -rf docs/ && jsdoc --configure .jsdoc.json --verbose",
    "build-site": "react-scripts build && cp -r lib build/lib && cp -r docs build/docs",
    "build": "npm run build:lib && npm run build-docs && npm run build-site",
    "deploy": "npm run build && surge -p build -d sandpitjs.com"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/superhighfives/sandpit.git"
  },
  "keywords": [
    "sketch",
    "creative",
    "canvas",
    "processing",
    "animation"
  ],
  "author": "Charlie Gleason (http://charliegleason.com/)"
}
