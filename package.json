  "description": "A playground for creative coding using JavaScript and the canvas element",
  "main": "build/index.js",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "color": "^1.0.3",
    "dat.gui": "^0.6.1",
    "debounce": "^1.0.0",
    "jsdoc": "^3.4.3",
    "minami": "^1.1.1",
    "queryfetch": "^1.2.1",
    "react-scripts": "0.8.4",
    "seedrandom": "^2.4.2",
